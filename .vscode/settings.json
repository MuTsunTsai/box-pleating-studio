{
	"latex-workshop.latex.recipe.default": "xelatex",
	"latex-workshop.latex.recipes": [
		{
			"name": "xelatex",
			"tools": [
				"xelatex",
			]
		}
	],
	"files.exclude": {
		"node_modules": true,
		"**/*.map": true,
		"**/debug.log": true,
		"docs/**/*.log": true,
		"**/*.aux": true,
		"**/*.nav": true,
		"**/*.out": true,
		"**/*.snm": true,
		"**/*.synctex.gz": true,
		"**/*.toc": true,
		"dist/index.html": true
	},
	"i18n-ally.enabledFrameworks": [
		"vue"
	],
	"i18n-ally.localesPaths": "src/locale",
	"i18n-ally.enabledParsers": [
		"json"
	],
	"i18n-ally.sourceLanguage": "en",
	"i18n-ally.keystyle": "nested",
	"i18n-ally.indent": 4,
	"i18n-ally.tabStyle": "tab",
	"vetur.experimental.templateInterpolationService": true,
	"cSpell.words": [
		"bpstudio",
		"configurator",
		"fullscreen",
		"gops",
		"gtag",
		"kamiya",
		"mixins",
		"notranslate",
		"osme",
		"overlappings",
		"partitioner",
		"precache",
		"precaching",
		"scrollable",
		"selectables",
		"serializable",
		"tsai",
		"tsun"
	],
	"cSpell.ignoreRegExpList": [
		"/[A-Z]+(?![a-z])/g"
	],
	"cSpell.ignorePaths": [
		"**/package*.json",
		".vscode",
	]
}
